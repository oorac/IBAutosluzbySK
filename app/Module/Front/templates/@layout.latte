	<div class="three_box">
		<div class="container">
			<div class="row">
				<div class="col-md-4">
					<div class="box_text">
						<h3>Financovanie</h3>
						<i><img src="/images/thr3.png" alt="#"/></i>
						<p>{if isset($blokyBase['Financovanie']['text'])}{strip_tags($blokyBase['Financovanie']['text'])}{/if}</p>
					</div>
				</div>
				<div class="col-md-4">
					<div class="box_text">
						<h3>Dovoz áut</h3>
